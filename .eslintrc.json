{
  "extends": [
    "airbnb-base",
    "eslint:recommended"
  ],
  "rules": {
    "semi": [2, "always"],
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "no-unexpected-multiline": 2,
    "linebreak-style": [2, "windows"],
    "no-plusplus": "off",
    "no-console": "off"
  },
  "globals": {
    "remConfig": true,
    "TranslatableError": true,
    "rem": true
  }
}